{
    "name": "pandadoc/php-client",
    "version": "5.2.0",
    "description": "The Official PandaDoc PHP client SDK",
    "keywords": [
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://developers.pandadoc.com",
    "license": "MIT",
    "authors": [
        {
            "name": "PandaDoc",
            "homepage": "https://developers.pandadoc.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "autoload": {
        "psr-4": { "PandaDoc\\Client\\" : "src/" }
    }
}
